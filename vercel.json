{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "dist/control-ganado/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/dist/control-ganado/assets/$1" },
    { "src": "/favicon.ico", "dest": "/dist/control-ganado/favicon.ico" },
    { "src": "/main.js", "dest": "/dist/control-ganado/main.js" },
    { "src": "/polyfills.js", "dest": "/dist/control-ganado/polyfills.js" },
    { "src": "/styles.css", "dest": "/dist/control-ganado/styles.css" },
    { "src": "/(.*)", "dest": "/dist/control-ganado/index.html" }
  ]
}
